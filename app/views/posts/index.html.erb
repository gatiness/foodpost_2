<p id="notice"><%= notice %></p>

<div class="main">

<table border="1">
  <thead>
    <tr>
      <th>タイトル</th>
      <th>内容</th>
      <th>画像</th>
      <th>ラベル</th>
      <th>投稿者</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id = "search">
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.content %></td>
        <td>
          <% if post.image.attached? %>
            <%= image_tag (post.image), height: '80' %> 
          <% end %>
        </td>
        <td><% post.labels.each do |label| %>
            <%= label.name %>
          <% end %></td>
        <td><%= link_to post.user.name, user_path(post.user.id) %></td>
        <td><%= link_to '詳細', post %></td>
        <% if post.user == current_user %>
          <td><%= link_to '編集', edit_post_path(post) %></td>
          <td><%= link_to '削除', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %> 
    <% end %>  
      </tr>
  </tbody>
</table>

<br>

